<% resp.forEach(blog => { %>
 <% if (blog.id===id) { %>
    <h1>
        <%= blog.title %>
    </h1>
    <img src="/upload/<%= blog.img %>" alt="<%= blog.title %>" />
    <p>
        <%= blog.des %>
    </p>
				<button data-test="<%= blog.id %>">Delete</button>
 <% } %>
<% }) %>
<a href="/blog">Blogs</a>
<a href="/blog/createblog">Create Blog</a>
<a href="/users/logout">Log out</a>
